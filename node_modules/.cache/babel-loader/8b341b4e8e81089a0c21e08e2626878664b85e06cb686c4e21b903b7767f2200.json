-text-field\n            v-model=\"date\"\n            label=\"Geburtsdatum\"\n            append-icon=\"mdi-calendar\"\n            readonly\n            v-bind=\"attrs\"\n            v-on=\"on\"\n            rounded outlined\n          ></v-text-field>\n        </template>\n        <v-date-picker\n          v-model=\"date\"\n          @input=\"menu2 = false\"\n        ></v-date-picker>\n      </v-menu>\n            </v-col>\n          </v-row>\n          </v-form>\n          </v-col>\n          \n          </div>\n          <div v-if=\"show === 2\">\n          <h1>test</h1></div>\n          \n        </v-col>\n\n        </v-row>\n        \n        \n</v-container>\n</v-app>\n</template>\n<script>\n  import NavBar from \"../components/NavBar.vue\";\n\n export default {\n  components: {\n      NavBar,\n  },\n    data: () => ({\n        username: \"Mia\",\n        date: (new Date(Date.now() - (new Date()).getTimezoneOffset() * 60000)).toISOString().substr(0, 10),\n        menu: false,\n        show: 1,\n        anrede: [\"Herr\", \"Frau\"],\n        items: [\n      { title: 'Meine Daten' },\n      { title: 'Meine Bestellungen' },\n      { title: 'Adressbuch' },\n      { title: 'Wunschliste' },\n      { title: 'Passwort ändern' },\n    ]\n  })}\n  </script>\n  <style>\n  body {\n    font-size: 16px ;\n    font-weight: 400 ;\n}\n\n.paddingTop100{\n    padding-top: 100px !important;\n  }\n  .alignleft {\n    text-align: left !important;\n  }\n\n  .whiteFont{\n    color: white;\n  }\n  .v-list-item{\n    border-bottom: .0625rem solid #ebebeb;\n  }\n\n  .v-list-item__title.bold-hover{\n    font-weight: bold;\n  }\n  .fullGrowX{\n    width: 100%;\n    background-color: white !important;\n    \n  }\n  .shadowCenter{\n    box-shadow: 0 2px 8px 0 rgb(88 88 88 / 32%)\n  }\n  @media screen and (min-width: 375px){\n    .variableFontSize {\n    font-size: calc(24px + 24*(100vw - 375px)/ 1545);\n  }\n}\n.v-list-item {\n  border-bottom: 0;\n}\n  </style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}